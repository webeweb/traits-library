{
    "name": "webeweb/traits-library",
    "type": "library",
    "description": "Traits collection",
    "keywords": [
        "library",
        "traits",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/traits-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Traits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Traits\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/traits-library/issues"
    }
}
